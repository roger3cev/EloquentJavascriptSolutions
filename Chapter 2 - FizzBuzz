// FizzBuzz.
  const range = (start, end) => Array.from({ length: (end - start + 1) }, (_, k) => k + start)
  
  const fizzBuzz = (number) => {
    const fizz = number % 3 == 0
    const buzz = number % 5 == 0
    if (fizz) return "Fizz"
    if (buzz) return "Buzz"
    if (fizz && buzz) return "FizzBuzz"
    return String(number)
  }
  
  range(1, 100).map(x => fizzBuzz(x)).forEach(x => console.log(x))
  

  
  